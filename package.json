{
  "name": "trpc-formdata",
  "version": "0.2.4",
  "description": "A tRPC link for handling FormData with file uploads and validation",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/dugajean/trpc-formdata.git"
  },
  "license": "MIT",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js"
    },
    "./zod": {
      "types": "./dist/zod.d.ts",
      "import": "./dist/zod.js"
    }
  },
  "files": [
